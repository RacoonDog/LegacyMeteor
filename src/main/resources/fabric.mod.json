{
  "schemaVersion": 1,
  "id": "legacy-api",
  "version": "${version}",
  "name": "LegacyAPI",
  "description": "Allows rudimentary usage of outdated Meteor Client addons.",
  "authors": [ "Crosby" ],
  "contact": {
    "repo": "https://github.com/RacoonDog/LegacyAPI"
  },
  "icon": "assets/legacy-api/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "io.github.racoondog.legacyapi.LegacyAPIAddon"
    ]
  },
  "mixins": [
    "legacy-api.mixins.json"
  ],
  "accessWidener": "legacy-api.accesswidener",
  "custom": {
    "modmenu": {
      "badges": [ "library" ]
    }
  },
  "depends": {
    "java": ">=17",
    "minecraft": ">=${mc_version}",
    "meteor-client": "*"
  }
}
