{
  "schemaVersion": 1,
  "id": "legacy-meteor",
  "version": "${version}",
  "name": "LegacyMeteor",
  "description": "Allows rudimentary usage of outdated Meteor Client addons.",
  "authors": [ "Crosby" ],
  "contact": {
    "repo": "https://github.com/RacoonDog/LegacyMeteor",
    "sources": "https://github.com/RacoonDog/LegacyMeteor",
    "issues": "https://github.com/RacoonDog/LegacyMeteor/issues"
  },
  "license": "ARR",
  "icon": "assets/legacy-meteor/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "io.github.racoondog.legacymeteor.LegacyMeteorAddon"
    ]
  },
  "mixins": [
    "legacy-meteor.mixins.json"
  ],
  "custom": {
    "modmenu": {
      "badges": [ "library", "meteor" ]
    }
  },
  "depends": {
    "java": ">=17",
    "fabricloader": ">=0.12",
    "minecraft": ">=${mc_version}",
    "meteor-shared-addon-utils": "*",
    "meteor-client": "*"
  }
}
